[user]
  name = Sam Soffes
  email = sam@soff.es
  signingkey = 8F60329C
[github]
  user = soffes
[alias]
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  up = pull --rebase
  punch = blame
  pish = push
	tpush = push
[color]
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = ~/.gitignore
[apply]
  whitespace = nowarn
[merge]
  tool = kdiff3
[difftool]
  prompt = false
[push]
  default = current
[credential]
  helper = osxkeychain
[ghi]
  token = !security find-internet-password -a `git config github.user` -s github.com -l 'ghi token' -w
